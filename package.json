{
	"name": "laravel-routes",
	"displayName": "Laravel Routes",
	"description": "Show your laravel routes as you didn't seen it before with all the details",
	"author": {
		"name": "mahmoud shahin",
		"email": "mahmoudshahin1111@gmail.com",
		"url": "https://mahmoudshahin.net"
	},
	"icon": "https://github.com/mahmoudshahin1111/laravel-routes/blob/master/assets/logo.png",
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:laravel-routes.start"
	],
	"main": "./dist/extension.js",
	
	"repository": {"url": "https://github.com/mahmoudshahin1111/laravel-routes"},
	"contributes": {
		"commands": [
			{
				"command": "laravel-routes.start",
				"title": "LaravelRoutes: Start"
			}
		],
		"configuration":{
			"title": "Laravel Routes",
			"properties": {
				"routes.RoutesFolderPath":{
					"type":"string",
					"default":"routes"
				},
				"routes.ProvidersFolderPath":{
					"type":"string",
					"default":"app/Providers"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"ts-loader": "^9.2.5",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"php-parser": "^3.0.3"
	}
}
